{
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.3.*",
        "vlucas/phpdotenv": "~2.2",
        "dusterio/lumen-passport": "^0.1.0",
        "spatie/laravel-fractal": "^2.0",
        "symfony/var-dumper": "^3.1",
        "psy/psysh": "@stable",
        "classpreloader/classpreloader": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Flipbox\\ConsoleSupport\\": "src/ConsoleSupport/",
            "Flipbox\\Fractal\\": "src/Fractal/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
